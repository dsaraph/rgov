<h1>Add a new service</h1>
<%= form_for @service do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
    <%= f.text_field :service_name %>  
  </div>
  <div class="field">
    <%= f.text_field :service_url %>  
  </div>
  <div class="field">
    <%= f.text_area :service_desc %>  
  </div>
  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>
